{
  "name": "webpack-typescript-starter",
  "version": "0.0.1",
  "description": "Tetris game built with typescript",
  "homepage": "https://DerekJi.github.io/ts-tetris",
  "main": "index",
  "typings": "dist/index",
  "scripts": {
    "webpack": "webpack",
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.prod.config.js -p",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext",
    "test": "karma start karma.config.js"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "Juri Strumpflohner",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jasmine": "^2.8.7",
    "@types/node": "^7.0.0",
    "awesome-typescript-loader": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^4.3.0",
    "jasmine-core": "^3.1.0",
    "karma": "^6.3.16",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.0",
    "source-map": "0.7.2",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.17.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/types": "^7.10.4",
    "@types/jquery": "^3.5.0",
    "axios": "^0.19.2",
    "cryptiles": "^4.1.3",
    "hoek": "^6.1.3",
    "jquery": "^3.5.1",
    "serialize-javascript": "^4.0.0"
  }
}
